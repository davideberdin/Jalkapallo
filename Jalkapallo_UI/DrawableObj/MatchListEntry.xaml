<UserControl x:Class="Jalkapallo_UI.DrawableObj.MatchListEntry"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="60" d:DesignWidth="450">

    <Grid x:Name="SuperiorGrid" Height="54">
        <Grid x:Name="ContextGrid" Width="445" HorizontalAlignment="Center" Margin="5,9,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="450*" />
                <ColumnDefinition Width="176*" />

            </Grid.ColumnDefinitions>
            <TextBlock x:Name="Squadre" Text="Casa-Ospite" Margin="15,0,0,0" FontSize="25" />
            <TextBlock x:Name="Risultato" Text="Risultato" FontSize="25" Grid.Column="1" Margin="0,0,8,0" Width="99" HorizontalAlignment="Right" />
        </Grid>
    </Grid>
</UserControl>
